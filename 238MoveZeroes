class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int i=0;
        int j=0;
        int n=nums.size();
        while(i<n && nums[i]!=0) {
            i++;
            j++;
        }
        j++;
        while(j<n){
            if(nums[j]==0) j++;
            else{
                swap(nums[i],nums[j]);
                while(nums[i]!=0) i++;
            }
        }

    }
};